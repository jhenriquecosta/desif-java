<hibernate-mapping>
  <class name="br.gov.pbh.desif.model.pojo.EventosContabeis" schema="PUBLIC" table="EVENTOS_CONTABEIS">
    <id name="id" type="int">
      <column name="IDN_EVNT_CNTB"/>
      <generator class="assigned"/>
    </id>
    <property name="descricaoEvento" type="string">
      <column name="DES_EVNT_CNTB" not-null="true"/>
    </property>
  </class>
</hibernate-mapping>
